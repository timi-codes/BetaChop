{
  "name": "betachop",
  "version": "1.0.0",
  "description": "A meal booking app",
  "main": "API/index.js",
  "engines": {
    "node": "11.6.0",
    "npm": "6.5.0-next.0"
  },
  "scripts": {
    "ui": "run-script-os",
    "ui:win32": "browser-sync ui --server --files 'UI/**/*.css, UI/**/*.html, UI/**/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
    "ui:darwin:linux": "browser-sync ui --server --files 'UI/**/*.css, UI/**/*.html, UI/**/*.js, !node_modules/**/*' --directory --port 7777 --browser 'Firefox Developer Edition'",
    "start-dev": "nodemon --exec babel-node API/index.js",
    "heroku-postbuild": "NODE_ENV=production && npm run build",
    "start": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all && node dist/index.js",
    "pretest": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && sequelize db:seed:all --env test",
    "test": "set NODE_ENV=test && nyc --require @babel/register ---reporter=html --reporter=text mocha API/test/**.test.js --timeout 20000 --exit",
    "build-babel": "babel ./API --out-dir dist/ --copy-files --ignore ./node_modules,./coverage,./.babelrc,./package.json,./npm-debug.log,./.travis.yml,./.eslintrc.js,./UI,./README.md",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-babel",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "client-server": "webpack-dev-server --mode development ./client/index.js --output ./client/dist/main.js --open",
    "build-wp": "webpack --mode production ./client/index.js --output ./client/dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timi-codes/BetaChop.git"
  },
  "keywords": [
    "food",
    "chop",
    "betachop"
  ],
  "author": "timi-codes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timi-codes/BetaChop/issues"
  },
  "homepage": "https://github.com/timi-codes/BetaChop#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-thunk": "^1.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "swagger-jsdoc": "^1.3.0",
    "swagger-ui-express": "^4.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "run-script-os": "^1.0.3",
    "should": "^13.2.3"
  }
}
